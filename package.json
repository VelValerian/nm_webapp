  "main": "index.js",
  "scripts": {
    "dev:css": "sass --watch --source-map assets/scss/styles.scss static/css/styles.css",
    "build:css": "sass --no-source-map assets/scss/styles.scss static/css/styles.css && postcss static/css/styles.css -o static/css/styles.css",
    "dev:js": "chokidar 'assets/js/**/*.js' -c 'cp assets/js/main.js static/js/main.js'",
    "build:js": "cp assets/js/main.js static/js/main.js",
    "dev": "npm-run-all --parallel dev:*",
    "build": "npm-run-all build:*",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
